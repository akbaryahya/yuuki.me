{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "@UT/*": ["src/util/*"],
      "@SV/*": ["src/server/*"],
      "@CMD/*": ["src/commands/*"],
    }
  },
  "include": ["src/**/*"]
}
